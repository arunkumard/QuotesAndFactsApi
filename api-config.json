		"restrictions": [{
			"method": "*",
			"type": "throttle",
			"config": {
				"period": "hour",
				"messages": 60,
				"per": 2
			}
		}]
	},
	"applicationQuota": {
		"restrictions": [{
			"method": "*",
			"type": "throttlemb",
			"config": {
				"period": "hour",
				"mb": 55,
				"per": 1
			}
		}]
	}
