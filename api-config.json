	"backendBasepath": "http://quotesapi--arunkumard.repl.co",
	"tags": {
		"nextStage": ["preprod"],
		"status": ["Beta"],
		"apiType": ["Quotes Facts"],
		"publisher": ["test"]
	},
	"securityProfiles": [{
		"name": "_default",
		"isDefault": true,
		"devices": [{
			"name": "API Key",
			"type": "apiKey",
			"order": 0,
			"properties": {
				"apiKeyFieldName": "KeyId",
				"takeFrom": "HEADER",
				"removeCredentialsOnSuccess": "false"
			}
		}]
	}],
	"clientOrganizations": [
		"ALL"
	],
	"applications": [{
		"name": "amplify_central"
	}],
